{
  "name": "redux-form-doc-version-checker",
  "version": "1.0.5",
  "description":
    "A widget to notify people if they are not reading the most recent docs.",
  "main": "dist/doc-check.cjs.js",
  "jsnext:main": "dist/doc-check.es.js",
  "module": "dist/doc-check.es.js",
  "files": ["dist"],
  "scripts": {
    "start": "nps",
    "precommit":
      "lint-staged && opt --in pre-commit --exec \"npm start validate\""
  },
  "author":
    "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/redux-form-doc-version-checker.git"
  },
  "bugs": {
    "url": "https://github.com/erikras/redux-form-doc-version-checker/issues"
  },
  "homepage":
    "https://github.com/erikras/redux-form-doc-version-checker#readme",
  "lint-staged": {
    "*.+(js|json)": ["prettier-eslint --write", "git add"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bundlesize": "^0.14.4",
    "eslint": "^4.6.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.54.1",
    "lint-staged": "^4.1.3",
    "nps": "^5.7.1",
    "nps-utils": "^1.3.0",
    "prettier": "^1.6.1",
    "prettier-eslint-cli": "^4.3.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "webpack": "^3.5.6"
  },
  "bundlesize": [
    {
      "path": "dist/doc-check.umd.min.js",
      "threshold": "20kB"
    },
    {
      "path": "dist/doc-check.es.js",
      "threshold": "40kB"
    },
    {
      "path": "dist/doc-check.cjs.js",
      "threshold": "40kB"
    }
  ],
  "dependencies": {
    "http-client": "^4.3.1",
    "js-cookie": "^2.1.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "semver": "^5.4.1",
    "styled-components": "^2.1.2"
  }
}
